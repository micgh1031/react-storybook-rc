    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "convert-units": "^2.2.0",
    "fade-props": "^2.1.0",
    "lodash": "^4.17.4",
    "mapbox-gl": "^0.43.0",
    "mixpanel-browser": "^2.18.0",
    "normalizr": "^3.2.4",
    "prop-types": "^15.6.0",
    "react": "^15.5.4",
    "react-autocomplete": "^1.7.2",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.4.0",
    "react-layout-components": "^3.0.6",
    "react-mapbox-gl": "^3.4.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-tabs": "^2.1.1",
    "react-transition-group": "^2.2.1",
    "recharts": "0.22.4",
    "redux": "^3.7.2",
    "redux-form": "^6.7.0",
    "redux-thunk": "^2.2.0",
    "safe-eval": "^0.3.0",
    "source-map-explorer": "^1.5.0",
    "url-search-params-polyfill": "^2.0.1"
  },
  "devDependencies": {
    "babel-plugin-react-intl": "^2.3.1",
    "babel-preset-react-app": "^3.1.0",
    "postcss-cli": "^4.1.1",
    "postcss-cssnext": "^3.0.2",
    "react-scripts": "1.0.17",
    "react-test-renderer": "^16.2.0"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "react-scripts start",
    "build": "react-scripts build && postcss -u postcss-cssnext -r build/**/*.css",
    "build:production": "REACT_APP_ENV=production react-scripts build && postcss -u postcss-cssnext -r build/**/*.css",
    "deploy": "yarn build && cp .surgeignore build/.surgeignore && surge --project ./build --domain https://reforestum-app-staging.surge.sh",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "translate": "rm -rf ./__i18n__ && NODE_ENV=production babel ./src >/dev/null && node scripts/translate.js && rm -rf ./__i18n__"
  }
}
